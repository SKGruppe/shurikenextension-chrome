{
    "name": "Shuriken",
    "version": "1.0.0",
    "description": "Privacy respecting search engine extension, with functionality",
    "manifest_version": 3,
    "author": "SKG",
    "action":{
        "default_popup": "popup.html",
        "default_title": "Shuriken"
    },
    "chrome_url_overrides": {
      "newtab": "start.html"
    }
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "64.0"
      }
    },
    "chrome_settings_overrides": {
      "search_provider": {
        "name": "Shuriken",
        "keyword": "@sk, sse",
        "search_url": "https://shuriken.pm/search.php?q={searchTerms}",
        "favicon_url": "https://shuriken.pm/favicon.ico",
        "is_default": true
      }
    }
}
